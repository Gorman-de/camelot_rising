cyoAdventure = function(a) {
    ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length-1];
    e.noCounter += 1
    
    // eminence
    if(ans == 9041 || ans == 9374 || ans == 9377){
        eminence += 1
    }
    if(ans == 999999999){
        eminence -= 1
    }
    // south_support
    if(ans == 8954 || ans == 9059 || ans == 9346 || ans == 9291){
        south_support += 1
    }
    if(ans == 9341){
        south_support += 2
    }
    if(ans == 999999999){
        south_support -= 1
    }
    // cult_support
    if(ans == 9042 || ans == 8961 || ans == 9296){
        cult_support += 1
    }
    if(ans == 9358){
        cult_support += 2
    }
    if(ans == 9378){
        cult_support -= 1
    }
    if(ans == 8958 || ans == 9047 || ans == 9048 ){
        openSurrogates();
    }
    // achievements
    if(ans == 9055 || ans == 9358 || ans == 9208 || ans == 9296){
        achievements += 1
    }
    // economy
    if(ans == 9296){
        economy -= 1
    }
    if(ans == 9055){
        economy -= 2
    }

    // Question CYOA
    if(eminence > 0 && (e.noCounter == 2)){
        campaignTrail_temp.questions_json[2] = tunnel(9301);   
    }
    if(south_support > 0 && (e.noCounter == 2)){
        campaignTrail_temp.questions_json[2] = tunnel(9317);   
    }
    if(e.player_answers.includes(8954) && (e.noCounter == 5)){
        campaignTrail_temp.questions_json[5] = tunnel(9335);   
    }
    if(e.player_answers.includes(8964) && (south_support + cult_support) >= 3 && (e.noCounter == 6)){
        campaignTrail_temp.questions_json[6] = tunnel(9290);   
    }
    if(e.player_answers.includes(9056) && (e.noCounter == 6)){
        campaignTrail_temp.questions_json[6] = tunnel(9354);   
    }
    if(e.player_answers.includes(9055) && (e.noCounter == 6)){
        campaignTrail_temp.questions_json[6] = tunnel(9355);   
    }
    if((e.player_answers.includes(9056) || (cult_support > south_support)) && (e.noCounter == 7)){
        campaignTrail_temp.questions_json[7] = tunnel(9406);   
    }

}

function initializeTooltips() {
    var tooltipContainers = document.querySelectorAll('.tooltip-container');
    tooltipContainers.forEach(function(container) {
        var tooltip = container.querySelector('.tooltip');
        container.addEventListener('mouseover', function(event) {
            if (event.target === container) {
                tooltip.style.opacity = '1';
                tooltip.style.display = 'block'; // Show tooltip and set display to block
            }
        });
        container.addEventListener('mouseout', function(event) {
            if (event.target === container) {
                tooltip.style.opacity = '0';
                tooltip.style.display = 'none'; // Hide tooltip and set display to none
            }
        });
    });
}

